<h1>Contents#new</h1>
<p>Find me in app/views/buttafly/contents/new.html.erb</p>

<%= form_for @originable, :html => {:multipart => true} do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :flat_file %>
    <%= f.file_field :flat_file %>
  </p>
  <p><%= f.submit %></p>
<% end %>